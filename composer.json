{
    "name": "eup/base-repo",
    "description": "Base repository implementation for Laravel",
    "keywords": [
      "laravel",
      "repository",
      "repository pattern",
      "eloquent",
      "repositories"
    ],
    "license": "BSD-2-Clause",
    "authors": [
      {
        "name": "TienPham",
        "email": "tienpv@eupgroup.net"
      }
    ],
    "require": {
      "php": ">=7.1",
      "illuminate/database": "~5.1"
    },
    "require-dev": {
      "phpunit/phpunit": "^4.8",
      "mockery/mockery": "^0.9.4"
    },
    "autoload": {
      "psr-4": {
        "Base\\": "src/"
      }
    },
    "autoload-dev": {
      "files": [
        "tests/TestFunctions.php"
      ],
      "psr-4": {
        "Base\\Test\\": "tests"
      }
    },
    "extra": {
      "laravel": {
        "providers": [
            "Base\\RepositoryServiceProvider"
        ]
      }
    },
    "archive" : {
      "exclude": [
        "/tests"
      ]
    }
  }
  